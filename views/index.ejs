      $(document).ready(function() {
        Crittercism.init({
          appId: '54d7b0a351de5e9f042ed804', // Example: 47cc67093475061e3d95369d
          appVersion: '1.0', // Developer-provided application version
        });
      });
      localStorage.slideIndex = 0;
    </script>    
</body>
</html>